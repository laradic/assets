{
    "name": "laradic/assets",
    "description": "Laravel 5 Asset Manager. Fully featured, flexible, extendable and toroughly tested and documented.",
    "license": "MIT",
    "authors": [{"name": "Robin Radic", "email": "robin@laradic.nl", "homepage": "http://radic.nl", "role": "Developer"}],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "~5.4",
        "laradic/support": "~1.0",
        "laradic/filesystem": "~1.0",
        "laradic/dependency-sorter": "~1.0",
        "kriswallsmith/assetic": "~1.2"
    },
    "require-dev": {
        "laradic/testing": "~1.0.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.8 || ^5.6",

        "leafo/lessphp": "^0.3.7",
        "leafo/scssphp": "~0.1",
        "meenie/javascript-packer": "^1.1",
        "mrclay/minify": "<2.3",
        "natxet/cssmin": "3.0.4",
        "patchwork/jsqueeze": "~1.0|~2.0",
        "ptachoire/cssembed": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Laradic\\Assets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laradic\\Tests\\Assets\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
